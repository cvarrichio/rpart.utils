vectorize<-function(fun,type=0)
{
function(...)
{
cols<-cbind(...)
if((ncol(cols)>1) | (type==1 & (nrow(cols) > 1)))
apply(cols,1,function (x) Reduce(fun,unlist(x)))
else
Reduce(fun,cols[,1])
}
}
